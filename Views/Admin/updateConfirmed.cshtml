                            <li><a href="~/Admin/lawyerLogOut">log out</a></li>

                        </ul>
                    </nav>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
    </header>
    <div class="content">
        <div class="col-sm-5" style="margin:0px; padding:0px">

            <div>
                <h3 style="font-family:Georgia; margin-left:8px; color:#a94442"><b>update clients</b></h3>
            </div>

            <div class="panel panel-danger" style="margin: 80px 0px 0px 80px">

            
                    <section id="main-content">
                        <section class="wrapper">
                      

                            <!-- BASIC FORM ELELEMNTS -->
                            <div class="row mt">
                                <div class="col-lg-12">
                                    <div class="form-panel">
                  

                                        <table class="table">

                                            <tr style="background-color:#f2dede;">
                                                <td style="text-align:center; color:#a94442"><b>client ID</b></td>
                                                <td style="text-align:center; color:#a94442"><b>client Name</b></td>
                                                <td style="text-align:center; color:#a94442"><b>client age</b></td>
                                               

                                                <td style="text-align:center; color:#a94442"><b>      </b></td>
                                            </tr>

                                            @foreach (var p in Model)
                                            {
                                                if (p.lawyer_id == Convert.ToInt32(Session["lawyerId"]))
                                                {
                                                    <tr>
                                                        <td style="text-align:center">@p.client_id</td>
                                                        <td style="text-align:center">@p.client_name</td>
                                                        <td style="text-align:center">@p.client_age</td>
                                                        

                                                        <td style="text-align:center">
                                                            <form action="/Admin/update/@p.client_id" onsubmit="return agree()">
                                                                <input type="submit" value="Update" style="border:none; border-radius:3px; color:purple;font-weight:600" class="btn-warning" />
                                                            </form>

                                                        </td>
                                                    </tr>
                                                }
                                            }

                                        </table>
                                    </div>
                                </div><!-- col-lg-12-->
                            </div><!-- /row -->
                        </section>
                    </section>
                </div>
            </div>
        </div>
    <footer>
