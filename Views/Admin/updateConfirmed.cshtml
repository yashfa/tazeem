    <script src="~/Content/js/html5shiv.js"></script>
    <link rel="stylesheet" media="screen" href="~/Content/css/ie.css">
    <![endif]-->
</head>
<body>
    <header>
        <div class="container_12">
            <div class="grid_12">
                <h1><a href="~/Home/index"><img src="~/Content/images/logo.png" alt=""></a> </h1>
                <div class="clear"></div>
                <div class="menu_block">
                    
                    <nav>
                        <ul class="sf-menu">
                            <li> </li>
                            <li> </li>
                            <li> </li>
